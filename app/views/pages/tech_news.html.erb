<main role="main" class="inner cover home-page">
  <h1 class="cover-heading">Rails News</h1>
    <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card tweet">
          <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
    </div>
</main>
